<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="uno.allen.db.mapper.TownRfcMapper">
	<insert id="insertTownRfc" parameterType="uno.allen.entity.TownRfc">
		insert into town_rfc
		(
		town_fk_rfc,town_rfc,sbj_id_tfk,sbj_name_tfk
		)
		values 
		(
		#{Town_FK_RFC},#{Town_RFC},#{SBJ_ID_TFK},#{SBJ_Name_TFK}
		)
	</insert>
</mapper>